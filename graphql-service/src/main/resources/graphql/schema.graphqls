type Query{
    clientByClentId(clientId: ID): ClientDTO
    categoryList: [CategoryDTO]
    category(categoryId: ID): CategoryDTO

}
type ClientDTO{
    clientId: ID!
    category: [CategoryDTO]
}
type CategoryDTO{
    categoryId: ID
    clientId: String
    categoryName: String
    categoryType: [CategoryTypeDTO]
}
type CategoryTypeDTO{
    typeId: ID
    clientId: String
    typeName: String
    productGroup(clientId: ID): [ProductGroupDTO]
}

type ProductGroupDTO{
    productGroupId: ID
    clientId: String
    productGroupName: String
    productGroupDescription: String
    picUrl: String
    colaKey: String
    colbKey: String
    productSectionList: [ProductSectionADTO]
    product(selectA:String, selectB:String): ProductDTO
}

type ProductSectionADTO{
    productGroupId: ID
    colaValue: String
    productSectionB:[ProductSectionBDTO]
}

type ProductSectionBDTO{
    productGroupId: ID
    colbValue: String
    product:ProductDTO
}

type ProductDTO{
    productId: ID
    clientId: String
    productName: String
    productDescription: String
    colaValue: String
    colbValue: String
    inventory: [InventoryDTO]
}

type InventoryDTO{
    inventoryId: ID
    clientId: String
    productId: Int
    onhandQty: Int
    availableQty: Int
    accountingQty: Int
}
